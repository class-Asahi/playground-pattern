{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: g++ build main.cc",
            "command": "/usr/bin/g++",
            "args": [
                "-g",
                "${workspaceFolder}/src/main.cc",
                "-o",
                "${workspaceFolder}/build/main"
            ],
            "options": {
                "cwd": "${workspaceFolder}/build"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "detail": "Task generated by Debugger."
        },
        {
            "label": "CMake: Run target Untitled",
            "type": "shell",
            "command": "/usr/bin/bash ${workspaceFolder}/scripts/play.sh",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": "$gcc"
        }
    ],
    "version": "2.0.0"
}